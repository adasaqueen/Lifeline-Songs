      <lines>And many son had father abraham</lines>
    </verse>
    <verse name="i1a">
      <lines>I am one of them and so are you</lines>
    </verse>
    <verse name="i1b">
      <lines>Right hand</lines>
    </verse>
    <verse name="i1c">
      <lines>Left hand</lines>
    </verse>
    <verse name="i1d">
      <lines>Right foot</lines>
    </verse>
    <verse name="i1e">
      <lines>Left Foot</lines>
    </verse>
    <verse name="i1f">
      <lines>Turn Around</lines>
    </verse>
